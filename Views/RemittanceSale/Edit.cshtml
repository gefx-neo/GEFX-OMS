    function RegisterEnterKey() {
        $('#table-transaction tbody input').on('keyup keypress', function (e) {
            var keyCode = e.keyCode || e.which;
            if (keyCode === 13) {
                e.preventDefault();

                $(this).blur();

                setTimeout(function () {
                    $("form").submit();
                }, 700);
            }
        });
    }

